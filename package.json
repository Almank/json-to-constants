{
	"name": "json-to-constants",
	"displayName": "JSON to CONSTANTS",
	"description": "Convert your JSON struct to constant variables.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:json-to-constants.json-to-const"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-to-constants.json-to-const",
				"title": "Convert JSON to constants"
			}
		],
		"menus": {
			"editor/context": [
				{
				"command": "json-to-constants.json-to-const",
				"group": "1_modification"
		  }
		]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
